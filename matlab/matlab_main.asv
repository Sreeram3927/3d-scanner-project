port = 'COM3';
baudrate = 9600;
cmd = 'n';

arduino = serialport(port, baudrate);
%arduino.Timeout = 10;
fopen(arduino);

scanData = zeros(70, 40, 'uint16');

for i = 1:5
 %   while arduino.NumBytesAvailable == 80; end
    recieved_data = fscanf(arduino, );
    scanData(i, :) = recieved_data(1:40);
  disp(recieved_data);
end

disp(scanData);

fclose(arduino);
delete(arduino);